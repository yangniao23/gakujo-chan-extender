{
  "name": "gakujo-chan-extender",
  "version": "0.64.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build && wxt build -b firefox",
    "build:chrome": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:server": "node test-server.mjs",
    "postinstall": "wxt prepare",
    "clean": "rm -rf .wxt .output dist chrome_sources/*.js firefox_sources/*.js"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.266",
    "@types/firefox-webext-browser": "^120.0.0",
    "@vitest/ui": "^4.0.18",
    "esbuild": "^0.20.0",
    "jsdom": "^28.1.0",
    "typescript": "^5.4.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wxt": "^0.20.17"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "es-toolkit": "^1.44.0",
    "otpauth": "^9.5.0",
    "webextension-polyfill": "^0.12.0"
  }
}
